{
	"makeReservation": {
		"restaurant": [{
			"ReceiveConfirmationEmail": {
				"from": ["Email","Person"],
				"where":{
					"and": {
						"email": ["member_services@opentable.com", "no-reply@comms.opentable.com", "no-reply@opentable.com"],
						"Person._id":"from_id"
					}
				}
			}
		}],
		"hotel": []

	},
	"makeAPayment": {
		"restaurant": [{
			"PayByCreditCard": {
				"from": ["Category","Transaction","TransactionHasCategory"],
				"where": {
					"and": {
						"category" :["Food and Beverage Store","Food and Drink", "Restaurants", "Fast Food"],
						"`Transaction`._id": "TransactionHasCategory.transaction_id",
						"Category._id":"TransactionHasCategory.category_id"
					}
				}
			}
		}],
		"gas": [{
			"PayByCreditCard": {
				"from": ["Category","Transaction","TransactionHasCategory"],
				"where":{
					"and": {
						"category": "Gas Stations"
					}
				}
			}
		}]

	},
	"initiateDiscussion": {
		"restaurant": [{
				"SendEmail": {
					"from": "Email",
					"where": {
						"or": {
							"textContent":	"KEYWORDS_FILE",
							"subject": "KEYWORDS_FILE"
						}
					}
				}
			}],
		"opera": [

		]
	},
	"writeInCalendar": {
		"restaurant": [{
			"reminder": {
				"from": "Event",
				"where": {
					"and": {
						"description": "KEYWORDS_FILE"
					}
				}
			}
		}]
	},
	"GoToEventReminder": {
		"restaurant": [{
				"ReceiveGoogleCalendarNotification": {
					"from": "Email",
					"where": {
						"from": "calendar-notification@google.com"
					}
				}
			}, {
				"ReceiveEmail": {
					"from": "Email",
					"where": {
						"or": {
							"textContent": "KEYWORDS_FILE",
							"subject": "KEYWORDS_FILE"
						}
					}
				}
			}
		]
	},

	"GoToThePlace": {
		"restaurant": [{
				"CallUberOrLyft": {
					"GetTicketReceiptEmail": {
						"from": "Email",
						"where": {
							"or": {
								"from": [
									"Uber Receipts", "<noreply@uber.com>"
								],
								"subject": "trip with Uber"

							}
						}
					}
				}
			}, {
				"GoByCar": {
					"GetGas": {
						"makeAPayment": "Gas"
					}
				}
			}

		]
	},
	"attendEatingOutEvent": {
		"restaurant": [
			{
				"PostOnFacebook": {
					"from": ["Feed", "PlaceHasCategory", "Category"],
					"where": {
						"and": {
							"category" :["Food and Beverage Store","Food and Drink", "restaurant", "Fast Food"],
							"Feed.place_id":"PlaceHasCategory.place_id",
							"Category._id":"PlaceHasCategory.category_id"
						}
					}
				}
		}]
	}

}